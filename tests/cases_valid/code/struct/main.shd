struct ValidFields {
    field1: f32,
    field2: i32,
    field3: u32,
}

struct FieldsWithoutLastComma {
    field1: f32,
    field2: f32
}

buf value = ValidFields(0., 0, 0u);

run {
    value = new_value();
}

fn new_value() -> ValidFields {
    var value = ValidFields(4., 3, 2u);
    return copy(value);
}

fn copy(value: ValidFields) -> ValidFields {
    return value;
}
