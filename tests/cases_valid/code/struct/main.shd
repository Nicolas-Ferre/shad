struct ValidFields {
    field1: f32,
    field2: i32,
    field3: u32,
    field4: bool,
}

struct FieldsWithoutLastComma {
    field1: f32,
    field2: f32
}

buf value = ValidFields(
    field1: 0.,
    field2: 0,
    field3: 0u,
    field4: false,
);

run {
    value = new_value();
}

fn new_value() -> ValidFields {
    var value = ValidFields(4., 3, 2u, true);
    return copy(value);
}

fn copy(value: ValidFields) -> ValidFields {
    return value;
}
